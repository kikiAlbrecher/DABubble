<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Wenn Datei oder Ordner existiert → nicht umleiten
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]

  # Alles andere geht an index.html → Angular übernimmt
  RewriteRule ^.*$ index.html [L]
</IfModule>